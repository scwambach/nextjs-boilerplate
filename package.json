{
  "name": "gatsby-sanity-starter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "lerna run build --parallel",
    "build-web": "(cd web && npm run build)",
    "build-studio": "(cd studio && npm run build)",
    "commit": "git add . && cz",
    "dev": "lerna run dev --parallel",
    "format": "lerna run format",
    "graphql-deploy": "lerna run graphql-deploy",
    "lint": "lerna run lint",
    "postinstall": "lerna bootstrap",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "lerna run start --parallel",
    "backup": "curl https://k702ll7a.api.sanity.io/v1/data/export/production/ > backup.ndjson",
    "sanity-deploy": "cd studio && yarn deployall && cd .."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scwambach/gatsby-starter.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/scwambach/gatsby-starter/issues"
  },
  "homepage": "https://github.com/scwambach/gatsby-starter#readme",
  "devDependencies": {
    "@lhci/server": "^0.3.14",
    "@sanity/cli": "^1.149.19",
    "babel-eslint": "^10.1.0",
    "cz-customizable": "^6.3.0",
    "lerna": "^3.22.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  }

{
  "name": "nextjs-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "dev": "next dev -p 3030",
    "dev:test": "concurrently \"next dev -p 3030\" \"yarn test:watch\"",
    "dev:storybook": "concurrently \"next dev -p 3030\" \"yarn storybook\"",
    "dev:fullapp": "concurrently \"next dev -p 3030\" \"yarn storybook\" \"yarn test:watch\"",
    "build": "next build",
    "start": "next start -p 3030",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "prepare": "husky install",
    "check-format": "prettier --write .",
    "check-lint": "eslint . --ext ts --ext tsx --ext js",
    "storybook": "storybook dev -p 6006 --no-open",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@phosphor-icons/react": "^2.0.15",
    "@picocss/pico": "^2.0.6",
    "@react-google-maps/api": "^2.19.3",
    "@types/node": "20.11.25",
    "@types/react-dom": "18.2.20",
    "dayjs": "^1.11.10",
    "eslint": "8.57.0",
    "eslint-config-next": "14.1.3",
    "markdown-it": "^14.0.0",
    "next": "14.1.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-paginate": "^8.2.0",
    "react-player": "^2.15.1",
    "react-select": "^5.8.0",
    "typescript": "5.4.2"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^7.6.17",
    "@storybook/addon-essentials": "^7.6.17",
    "@storybook/addon-interactions": "^7.6.17",
    "@storybook/addon-links": "^7.6.17",
    "@storybook/addon-onboarding": "^1.0.11",
    "@storybook/addon-storysource": "^7.6.17",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/blocks": "^7.6.17",
    "@storybook/nextjs": "^7.6.17",
    "@storybook/react": "^7.6.17",
    "@storybook/test": "^7.6.17",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/jest-dom": "6.4.2",
    "@testing-library/react": "14.2.1",
    "@types/jest": "29.5.12",
    "@types/markdown-it": "^13.0.7",
    "@types/react": "18.2.64",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "autoprefixer": "^10.4.18",
    "axe-core": "^4.8.4",
    "chromatic": "^11.0.4",
    "concurrently": "^8.2.2",
    "cypress": "^13.6.6",
    "cypress-axe": "^1.5.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-storybook": "^0.8.0",
    "husky": "^9.0.11",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "prettier": "^3.2.5",
    "sass": "^1.71.1",
    "storybook": "^7.6.17"
  },
  "packageManager": "yarn@4.1.0"
}

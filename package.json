{
  "name": "nextjs-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "dev": "next dev -p 3030",
    "dev:test": "concurrently \"next dev -p 3030\" \"npm run test:watch\"",
    "dev:storybook": "concurrently \"next dev -p 3030\" \"npm run storybook\"",
    "dev:fullapp": "concurrently \"next dev -p 3030\" \"npm run storybook\" \"npm run test:watch\"",
    "build": "next build",
    "start": "next start -p 3030",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "prepare": "husky install",
    "check-format": "prettier --write .",
    "check-lint": "eslint . --ext ts --ext tsx --ext js",
    "storybook": "storybook dev -p 6006 --no-open",
    "build-storybook": "storybook build"
  },
  "engines": {
    "yarn": "please-use-npm"
  },
  "dependencies": {
    "@phosphor-icons/react": "^2.0.15",
    "@picocss/pico": "^2.0.6",
    "@react-google-maps/api": "^2.19.3",
    "@types/node": "20.11.27",
    "@types/react-dom": "18.2.22",
    "@typescript-eslint/parser": "^7.2.0",
    "dayjs": "^1.11.10",
    "eslint": "8.57.0",
    "eslint-config-next": "14.1.3",
    "markdown-it": "^14.0.0",
    "next": "14.1.3",
    "next-auth": "^4.24.7",
    "nodemailer": "^6.9.12",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-paginate": "^8.2.0",
    "react-player": "^2.15.1",
    "react-select": "^5.8.0",
    "typescript": "5.4.2"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^8.0.0",
    "@storybook/addon-essentials": "^8.0.0",
    "@storybook/addon-interactions": "^8.0.0",
    "@storybook/addon-links": "^8.0.0",
    "@storybook/addon-onboarding": "^8.0.0",
    "@storybook/addon-storysource": "^8.0.0",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/blocks": "^8.0.0",
    "@storybook/nextjs": "^8.0.0",
    "@storybook/react": "^8.0.0",
    "@storybook/test": "^8.0.0",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/jest-dom": "6.4.2",
    "@testing-library/react": "14.2.1",
    "@types/jest": "29.5.12",
    "@types/markdown-it": "^13.0.7",
    "@types/react": "18.2.66",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "autoprefixer": "^10.4.18",
    "axe-core": "^4.8.4",
    "chromatic": "^11.0.8",
    "concurrently": "^8.2.2",
    "cypress": "^13.7.0",
    "cypress-axe": "^1.5.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-storybook": "^0.8.0",
    "husky": "^9.0.11",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "prettier": "^3.2.5",
    "sass": "^1.72.0",
    "storybook": "^8.0.0"
  }
}
